PUPPETEER_TIMEOUT=30000

# Настройки безопасности
JWT_SECRET=your-secret-key-here

# Настройки базы данных (если будет использоваться)
# DATABASE_URL=postgresql://user:password@localhost:5432/google_forms_automator

# Настройки прокси (если нужны)
