{
  "name": "google-forms-automator",
  "version": "1.0.0",
  "description": "Универсальный софт для автоматического заполнения Google Forms",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "analyze": "node scripts/analyze-form.js",
    "generate": "node scripts/generate-data.js",
    "template": "node scripts/create-template.js",
    "fill": "node scripts/fill-form.js",
    "automator": "node scripts/google-forms-automator.js"
  },
  "keywords": [
    "google-forms",
    "automation",
    "nodejs",
    "react"
  ],
  "author": "Ruslan Agaev",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "proxy-chain": "^2.5.9",
    "puppeteer": "^21.5.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
